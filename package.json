{
  "name": "@ovenwand/app",
  "version": "0.2.0",
  "type": "module",
  "scripts": {
    "// bin aliases": "",
    "svelte-kit": "node_modules/@sveltejs/kit/svelte-kit.js",
    "svelte-check": "node_modules/svelte-check/bin/svelte-check",
    "prettier": "node_modules/prettier/bin-prettier.js",
    "start-storybook": "node_modules/@storybook/svelte/bin/index.js",
    "build-storybook": "build-storybook",
    "// project scripts": "",
    "clean": "pnpx -y rimraf node_modules",
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "storybook": "NODE_OPTIONS=--openssl-legacy-provider start-storybook -p 6006 --no-open",
    "storybook:build": "NODE_OPTIONS=--openssl-legacy-provider build-storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@storybook/addon-actions": "^6.5.0-alpha.21",
    "@storybook/addon-essentials": "^6.5.0-alpha.21",
    "@storybook/addon-links": "^6.5.0-alpha.21",
    "@storybook/addon-svelte-csf": "^1.1.0",
    "@storybook/svelte": "^6.5.0-alpha.21",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "babel-loader": "^8.2.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-storybook": "^0.5.6",
    "eslint-plugin-svelte3": "^3.2.1",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "storybook-builder-vite": "^0.1.13",
    "svelte": "^3.46.2",
    "svelte-check": "^2.2.6",
    "svelte-loader": "^3.1.2",
    "svelte-preprocess": "^4.9.4",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {}
}
